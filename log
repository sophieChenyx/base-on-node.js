<div id="myalert" class="dev-alert">
  <div id="my_alert" class="inner">
    <!-- <a href="javascript:;" onclick="myalert_hide();" class="guan"></a> -->
    <h1></h1>
    <div class="btns"><a id="my_ok" href="javascript:void(0);" class="button ok">确定</a></div>
